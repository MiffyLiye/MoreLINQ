{
    "name": "MoreLinq.Core",
    "title": "MoreLinq (.NET Core)",
    "authors": [
        "MoreLINQ Developers"
    ],
    "tags": [
        "linq",
        "extensions"
    ],
    "version": "2.0.0-beta04-2",
    "buildOptions": {
        "debugType": "portable",
        "emitEntryPoint": false,
        "warningsAsErrors": true,
        "define": [
            "TRACE",
            "DEBUG",
            "MORELINQ",
            "NO_SERIALIZATION_ATTRIBUTES",
            "NO_EXCEPTION_SERIALIZATION",
            "NO_TRACING",
            "NO_COM"
        ],
        "xmlDoc": true
    },
    "frameworks": {
        "netstandard1.6": {
            "buildOptions": {
                "compile": {
                    "exclude": [
                        "ToDataTable.cs"
                    ]
                },
                "define": [
                    "NO_EXCEPTION_SERIALIZATION"
                ]
            },
            "dependencies": {
                "NETStandard.Library": "1.6.0"
            }
        }
    }
}